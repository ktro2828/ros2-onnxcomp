<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="onnx_path" default="path/to/model.onnx" description="Path to the ONNX model"/>
    <arg name="use_raw" default="false" description="Use raw image data"/>

    <arg name="input/image" default="/input/image" description="Input image topic"/>
    <arg name="output/feature" default="/output/feature" description="Output feature topic"/>

    <node pkg="onnxcomp" exec="onnxcomp_node" name="onnxcomp_node" output="screen">
        <param name="onnx_path" value="$(var onnx_path)"/>
        <param name="use_raw" value="$(var use_raw)"/>
        <remap from="~/input/image" to="$(var input/image)"/>
        <remap from="~/output/feature" to="$(var output/feature)"/>
    </node>
</launch>
